@model TimeTracking_Ui.Models.UserDetails
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br></br><br></br>
<h2>Check the UserDetails.....</h2>


<div class="row">

    <form class="row g-3" asp-controller="UserDetails" asp-action="GetUser" method="get">
        <div class="col-md-6">
            <label asp-for="Id" class="form-label">ID</label>
            <input asp-for="Id" class="form-control" id="inputId">
        </div>
        <br></br><br></br>
        <div class="col-12">
            <button type="submit" class="btn btn-primary">GetUser</button>
        </div>
    </form>
</div>



